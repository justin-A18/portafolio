---
	import SectionContainer from '../utilities/SectionContainer.astro'
	import ExperienceItem from '../utilities/ExperienceItem.astro'
	import TitleSection from '../utilities/TitleSection.astro'
	
	import Briefcase from '../icons/Briefcase.astro';

	import {experience} from '../../data/experience'

---

<SectionContainer>
		
	<TitleSection>
		<Briefcase/> Experience
	</TitleSection>

	<ol class='relative border-s border-gray-500 dark:border-gray-700'>
		{
			experience.map((info) => <ExperienceItem {...info}/>)	
		}
	</ol>
</SectionContainer>

